let input = 7;
let c = 0;
let t = true;
while (true) {
    let g = false;
    if (t === true) {
        console.log(0);
        t = false;
        g = true;
    }
    if (t === false) {
        if (g === false) {
            console.log(1);
            t = true;
        }
    }
    c = c + 1;
    if (c === input) {
        break;
    }
}

